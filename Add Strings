import java.lang.*;
import java.io.*;
import java.util.*;

class Solution {
    public String addStrings(String num1, String num2) {
        int sum=0; 
        String ans="";
        int carry=0;
        int ad=0;
        int flag=0;
        if(num1.equals("") &&
num2.equals(""))
        { flag=1;
            return "";
        }
            
        else
        {if(flag==0 && num1.equals(num2) )
            return Integer.toString(2*Integer.parseInt(num1));
        else if(num1.length()==1 && num2.length()==1)
        {
            sum=Character.getNumericValue(num1.charAt(0))+Character.getNumericValue(num2.charAt(0));
            return Integer.toString(sum);
        }
        else if(num1.length()==num2.length() && flag==0)
        {
            
            for(int i=num1.length()-1;i>=0;i--)
            { //int carry=0;
              char a=num1.charAt(i);
             char b=num2.charAt(i);
                sum=Character.getNumericValue(a)+ Character.getNumericValue(b)+carry;
                //System.out.println(sum);
                if(i==0 && sum>9)
                {
                    ans=Integer.toString(sum)+ans;
                    
                }
                if(sum>9)
                {
                    ad=sum%10;
                    carry=sum/10;
                    //System.out.println(sum);
                    StringBuilder p= new StringBuilder();
                    
                    //System.out.println(p.reverse());
                    
                    ans=Integer.toString(ad)+ans;
                    /*p.append(ans);
                    ans=p.reverse();*/
                    
                }
                else
                {
                    ans=Integer.toString(sum)+ans;
                    carry=0;
                    
                }
                //System.out.println(carry);
            }
        }
        if(num1.length()>num2.length())
        {
            int diff=num1.length()-num2.length();
           // String formatted = 
             //ans=String.format("%0"+diff+"d", num2);
            //StringUtils.leftPad(ans,num1.length(), '0');
            String z=String.format("%0"+diff+"d",0);
                num2=z+num2;
            for(int i=num2.length()-1;i>=0;i--)
            { //int carry=0;
              char a=num1.charAt(i);
             char b=num2.charAt(i);
                sum=Character.getNumericValue(a)+ Character.getNumericValue(b)+carry;
               // System.out.println(sum);
                if(i==0 && sum>9)
                {
                    ans=Integer.toString(sum)+ans;
                    
                }
                else if(sum>9 && i!=0)
                {
                    ad=sum%10;
                    carry=sum/10;
                    //System.out.println(sum);
                    StringBuilder p= new StringBuilder();
                    
                    //System.out.println(p.reverse());
                    
                    ans=Integer.toString(ad)+ans;
                    /*p.append(ans);
                    ans=p.reverse();*/
                    
                }
                else 
                {
                    ans=Integer.toString(sum)+ans;
                    carry=0;
                    
                }
                //System.out.println(carry);
            }
            //System.out.println(ans);
            
        }
        if(num1.length()<num2.length())
        { //ans=num1.substring(num1.length(),num2.length());
            int diff=num2.length()-num1.length();
            String z=String.format("%0"+diff+"d",0);
            System.out.println(z);
                num1=z+num1;
            for(int i=num1.length()-1;i>=0;i--)
            { //int carry=0;
              char a=num1.charAt(i);
             char b=num2.charAt(i);
                sum=Character.getNumericValue(a)+ Character.getNumericValue(b)+carry;
                //System.out.println(sum);
                if(i==0 && sum>9)
                {
                    ans=Integer.toString(sum)+ans;
                    
                }
                if(sum>9)
                {
                    ad=sum%10;
                    carry=sum/10;
                    //System.out.println(sum);
                    StringBuilder p= new StringBuilder();
                    
                    //System.out.println(p.reverse());
                    
                    ans=Integer.toString(ad)+ans;
                    /*p.append(ans);
                    ans=p.reverse();*/
                    
                }
                else
                {
                    ans=Integer.toString(sum)+ans;
                    carry=0;
                    
                }
                //System.out.println(carry);
            }
            
        }
        
return ans.replaceFirst("^0+(?!$)", "");                      
    }
}
}
