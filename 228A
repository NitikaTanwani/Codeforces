#include <iostream>
using namespace std;
#include <bits/stdc++.h>

int main() {
	// your code goes here

	
	set<char> myset;
 
    int arr[4];
    for (int j=0;j<4;j++)
    {
    	cin>>arr[j];
    }
 
    for(int i=0;i<4;i++)
    {
            myset.insert(arr[i]);
        
    }
    cout<<(4-myset.size())<<endl;
	return 0;
}
